def binary_search(lst, target):
    low = 0
    high = len(lst) - 1

    while low <= high:
        mid = (low + high) // 2
        if lst[mid] < target:
            low = mid + 1
        elif lst[mid] > target:
            high = mid - 1
        else:
            return mid

    return low

def sort_sequence(sequence):
    lst = sequence.split()
    try:
        lst = [int(num) for num in lst]
    except ValueError:
        print("Ошибка: последовательность должна состоять только из чисел.")
        return

    lst.sort()
    return lst

# Ввод последовательности чисел
sequence = input("Введите последовательность чисел через пробел: ")
sorted_sequence = sort_sequence(sequence)
if not sorted_sequence:
    exit()

# Ввод числа
try:
    target = int(input("Введите любое число: "))
except ValueError:
    print("Ошибка: введено некорректное число.")
    exit()

# Поиск позиции элемента
position = binary_search(sorted_sequence, target)

if position >= len(sorted_sequence):
    print("Заданное число больше всех элементов последовательности.")
else:
    print(f"Позиция элемента, который меньше заданного числа {target}, "
          f"а следующий за ним больше или равен: {position}")
